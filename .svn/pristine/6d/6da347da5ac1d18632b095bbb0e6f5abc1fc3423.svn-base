   "source": [
    "# Example 1\n",
    "# evenly sampled x\n",
    "x = np.arange(0.5, 2.6, 0.01) #will not include 2.6\n",
    "def fun(x):\n",
    "    return np.exp(x)-5+ x**(-2)\n",
    "# red dashes and green dashes\n",
    "plt.plot(x, fun(x), 'b', x, 0/x, 'g')\n",
    "plt.xlabel('x', size=20)\n",
    "plt.ylabel('f(x)', size=20)\n",
    "plt.show()"
   ]
  },
